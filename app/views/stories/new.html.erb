<header>
	<h1><a href="/">The High Five Project</a></h1>
		<ul class="header">
			<li class="header"><a href="/">About</a></li>
			<li class="header"><a href="/stories">Stories</a></li> 
			<li class="header"><a href="/photos">Pictures</a></li>
			<li class="header"><a href="/history-of-the-high-five">History of the High Five</a></li> 
			<li class="header"><a href="/stories/new">Share Stories</a></li>
			<li class="header"><a href="/photos/new">Share Photos</a></li>
		</ul>
		
</header>
<div class="new">
	<%= form_for @story, :url => { :action => "create" }  do |f| %>
		<p><%= f.label :name %>:</p> 
		<p> <%= f.text_field :name %> </p>
		<p><%= f.label :location %>:</p>
		<p><%= f.text_field :location %></p>
		<p><%= f.label :story %>:</p>
		<p><%= f.text_area :story, :size => "60x12" %></p>
		<p><%= f.submit "Submit Your Story" %></p>
	<% end %>
</div>